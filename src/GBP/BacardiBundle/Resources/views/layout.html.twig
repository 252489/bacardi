<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8" />
	<title>{% block title %}Bacardi party!{% endblock %}</title>

	{% stylesheets 'bundles/gbpbacardi/css/*' filter='cssrewrite' output='css/compiled/main.css' %}
		<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}

	{% block stylesheets %}{% endblock %}

	{% block head %}
		<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
	{% endblock %}
</head>
<body>
{% block body %}{% endblock %}

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
{% block javascripts %}{% endblock %}
{% javascripts '@GBPBacardiBundle/Resources/public/js/*' output='js/compiled/main.js' %}
	<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
</body>
</html>